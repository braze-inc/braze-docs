{
	"trailingSlash": false,
	"rewrites": [
		{
			"source": "/docs/en/:path*",
			"destination": "/:path*"
		},
		{
			"source": "/docs/ko/:path*",
			"destination": "/:path*"
		},
		{
			"source": "/docs/:path*",
			"destination": "/:path*"
		},
		{
			"source": "/api/(.*)",
			"destination": "/api/404.html"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=604800, s-maxage=604800, stale-while-revalidate=86400, stale-if-error=86400"
				}
			]
		}
	]
}
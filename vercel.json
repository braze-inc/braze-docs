{
	"trailingSlash": false,
	"rewrites": [
		{
			"source": "/docs/:path*",
			"destination": "/:path*"
		},
		{
			"source": "/api/(.*)",
			"destination": "/api/404.html"
		},
		{
			"source": "*",
			"has": [
				{
					"type": "header",
					"key": "User-Agent",
					"value": ".*spider.*"
				}
			],
			"destination": "/blank.html"
		}
	],
	"github": {
		"silent": true
	},
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=604800, s-maxage=604800, stale-while-revalidate=86400, stale-if-error=86400"
				}
			]
		}
	]
}